{
  "config": {
    "network-interface": {
      "help": "options are ETHERNET,WIFI_ESP8266,MESH_LOWPAN_ND,MESH_THREAD",
      "value": "WIFI_ESP8266"
    },

    "log-port-tx": "PA_11",
    "log-port-rx": "PA_12",

    "esp8266-debug": false,
    "esp8266-tx": "PA_9",
    "esp8266-rx": "PA_10",
    "esp8266-reset": "NC",

    "device-button": "NC",
    "device-button-mode": 2,
    "device-aux": "NC",
    "device-status-led": "NC",
    "device-mains-switch": "NC",

    "panel-interface-bus-0": "NC",
    "panel-interface-bus-1": "NC",
    "panel-interface-bus-2": "NC",
    "panel-interface-bus-3": "NC",

    "electrical-outlet-plug-a-switch": "NC",
    "electrical-outlet-plug-a-sensor": "NC",
    "electrical-outlet-plug-b-switch": "NC",
    "electrical-outlet-plug-b-sensor": "NC"
  },
  "macros": [
    "MBED_STACK_STATS_ENABLED=1",
    "MBED_HEAP_STATS_ENABLED=1",
    "MBEDTLS_NO_DEFAULT_ENTROPY_SOURCES",
    "MBEDTLS_TEST_NULL_ENTROPY",
    "MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\""
  ],
  "target_overrides": {
    "*": {
      "target.features_add": [ "IPV4", "COMMON_PAL" ]
    },
    "NUCLEO_F401RE": {

      "esp8266-reset": "PB_6",

      "device-button": "PC_13",
      "device-aux": "PC_7",
      "device-status-led": "PA_5",

      "panel-interface-bus-0": "PB_3",
      "panel-interface-bus-1": "PB_5",
      "panel-interface-bus-2": "PB_4",
      "panel-interface-bus-3": "PB_10",

      "electrical-outlet-plug-b-switch": "PA_0",
      "electrical-outlet-plug-b-sensor": "PA_1"
    },
    "ARC_STM32F401RE": {

      "esp8266-reset": "PC_11",

      "device-button": "PC_4",
      "device-button-mode": 1,
      "device-aux": "PC_5",
      "device-status-led": "PC_3",
      "device-mains-switch": "PB_0",

      "panel-interface-bus-0": "PB_7",
      "panel-interface-bus-1": "PB_6",
      "panel-interface-bus-2": "PB_5",
      "panel-interface-bus-3": "PB_4",

      "electrical-outlet-plug-b-switch": "PB_8",
      "electrical-outlet-plug-b-sensor": "PA_6"
    }
  }
}